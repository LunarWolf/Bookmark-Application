<h1>Listing bookmarks</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Tags</th>
    <th>Shortener</th>
    <th></th>
  </tr>

<% @bookmarks.each do |bookmark| %>
  <tr>
    <td><%= bookmark.title %></td>
    <td><%= bookmark.url %></td>
    <td><%= bookmark.tags %></td>
    <td><%= bookmark.shortener %></td>
    <td><%= link_to 'Show', bookmark %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Bookmark', new_bookmark_path %>

<h1>Listing sites</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th></th>
    <th></th>
  </tr>

<% @sites.each do |site| %>
  <tr>
    <td><%= link_to site.title, site %></td>
    <td><%= site.url %></td>
  </tr>
<% end %>
</table>

